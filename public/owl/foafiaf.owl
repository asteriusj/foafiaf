#
# ENTITIES
#


:Entities  a       owl:Class , rdfs:Class ;
        rdfs:comment         "An entity (eg. person, organization, thing or physical artifact)." ;
        rdfs:isDefinedBy     : ;
        rdfs:label           "Entities" ;
        owl:equivalentClass  skos:Concept , rdfs:Resource ;
        vs:term_status       "" ;
        skos:definition      "Strategy maps identified entity(s)." .


:Concept  a          rdfs:Class , owl:Class ;
        rdfs:comment        "An idea or notion; a unit of thought." ;
        rdfs:isDefinedBy    foafiaf: ;
        rdfs:label          "Concept" ;
        rdfs:subClassOf     foafiaf:Entities ;
        owl:disjointWith    foafiaf:Event, foafiaf:Organization , foafiaf:Role , foafiaf:Person , foafiaf:Place  ;
        owl:equivalentClass skos:Concept ;
        vs:term_status      "" .

        
:Event  a            rdfs:Class , owl:Class ;
        rdfs:comment        "An event." ;
        rdfs:isDefinedBy    foafiaf: ;
        rdfs:label          "Event" ;
        rdfs:subClassOf     foafiaf:Entities ;
        owl:disjointWith    foafiaf:Concept, foafiaf:Organization , foafiaf:Role , foafiaf:Person , foafiaf:Place  ;
        owl:equivalentClass dbo:Event , schema:Event , org:Event , bio:Event , opencyc:Event , dct:Event , embel:Event , dul:Event , wikidata:Q1656682  ;
        vs:term_status      "" .


:Organization  a     rdfs:Class , owl:Class ;
        rdfs:comment        "An organization." ;
        rdfs:isDefinedBy    foafiaf: ;
        rdfs:label          "Organization" ;
        rdfs:subClassOf     foafiaf:Entities ;
        owl:disjointWith    foafiaf:Concept, foafiaf:Event , foafiaf:Role , foafiaf:Person , foafiaf:Place  ;
        owl:equivalentClass foaf:Organization , dbo:Organisation , schema:Organization , dul:SocialPerson , wikidata:Q43229 , bio:Organization ;
        vs:term_status    "" .


:Person  a            rdfs:Class , owl:Class ;
        rdfs:comment        "A place." ;
        rdfs:isDefinedBy    foafiaf: ;
        rdfs:label          "Person" ;
        rdfs:subClassOf     foafiaf:Entities , geo:SpatialThing ;
        owl:disjointWith    foafiaf:Concept , foafiaf:Event , foafiaf:Organization , foafiaf:Role  , foafiaf:Person ;
        owl:equivalentClass foaf:Person , dbo:Person , schema:Person , con:Person  ;
        vs:term_status      "" .  


:Place  a            rdfs:Class , owl:Class ;
        rdfs:comment        "A place." ;
        rdfs:isDefinedBy    foafiaf: ;
        rdfs:label          "Place" ;
        rdfs:subClassOf     foafiaf:Entities , geo:SpatialThing ;
        owl:disjointWith    foafiaf:Concept , foafiaf:Event , foafiaf:Organization , foafiaf:Role  , foafiaf:Person ;
        owl:equivalentClass dbo:Place , schema:Place  ;
        vs:term_status      "" .  


:Role  a            rdfs:Class , owl:Class ;
        rdfs:comment        "A role." ;
        rdfs:isDefinedBy    foafiaf: ;
        rdfs:label          "role" ;
        rdfs:subClassOf     geo:SpatialThing ;
        rdfs:subClassOf     foafiaf:Entities ; 
        owl:disjointWith    foafiaf:Concept , foafiaf:Event , foafiaf:Organization , foafiaf:Place  , foafiaf:Person ;
        owl:equivalentClass dbo:Place , schema:Place  ;        
        vs:term_status      "" .  
  
    
